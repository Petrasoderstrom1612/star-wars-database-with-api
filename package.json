{
  "name": "starwars",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check": "run-s lint typecheck type-coverage",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "json-server --watch data/db.json --port 3000 --delay 1500",
    "typecheck": "tsc -b",
    "type-coverage": "type-coverage -p ./tsconfig.app.json --detail --at-least 99"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "react-spinners": "^0.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "sass": "1.77.6",
    "type-coverage": "^2.29.7",
    "typescript": "~5.9.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "vite": "^7.2.4",
    "npm-run-all": "^4.1.5",
    "json-server": "^0.17.3"
  }
}
